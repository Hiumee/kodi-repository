<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="repository.hiumee" name="Hiumee's Repository" version="1.0.2" provider-name="Hiumee">
    <extension point="xbmc.addon.repository" name="Hiumee's Repository">
        <dir minversion="18.0.0">
            <info compressed="false">https://raw.githubusercontent.com/Hiumee/kodi-repository/main/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/Hiumee/kodi-repository/main/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/Hiumee/kodi-repository/main/zips/</datadir>
        </dir>
        <dir minversion="18.9.0">
            <info compressed="false">https://raw.githubusercontent.com/Hiumee/kodi-repository/main/matrix/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/Hiumee/kodi-repository/main/matrix/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/Hiumee/kodi-repository/main/matrix/zips/</datadir>
        </dir>
        <dir minversion="19.0.0">
            <info compressed="false">https://raw.githubusercontent.com/Hiumee/kodi-repository/main/matrix/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/Hiumee/kodi-repository/main/matrix/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/Hiumee/kodi-repository/main/matrix/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Hiumee's Addon Repository</summary>
        <description>Download, Install and Update addons</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
        </assets>
    </extension>
</addon>

<addon id="service.discord.richpresence"
  name="Discord Rich Presence"
  version="1.7.1"
  provider-name="Hiumee">
<requires>
  <import addon="xbmc.python" version="3.0.0"/>
  <import addon="script.module.requests"/>
</requires>
<extension point="xbmc.service" library="default.py" />
<extension point="xbmc.addon.metadata">
  <platform>all</platform>
  <source>https://github.com/Hiumee/service.discord.richpresence</source>
</extension>
</addon>
</addons>
